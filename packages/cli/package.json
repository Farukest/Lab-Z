{
  "name": "@labz/cli",
  "version": "1.0.0",
  "description": "Lab-Z CLI - Generate composable FHEVM smart contracts",
  "bin": {
    "labz": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts --clean",
    "dev": "tsup src/index.ts --format cjs --dts --watch",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@labz/core": "workspace:*",
    "@inquirer/prompts": "^7.2.1",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.1",
    "fs-extra": "^11.2.0",
    "boxen": "^8.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "fhevm",
    "fhe",
    "zama",
    "hardhat",
    "solidity",
    "cli",
    "encryption",
    "privacy"
  ]
}
