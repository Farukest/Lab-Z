{
  "name": "acl/voting-results",
  "version": "1.0.0",
  "description": "Share proposal vote counts with specific addresses via FHE.allow()",
  "author": "Lab-Z",
  "category": "acl",
  "tags": ["access-control", "permissions", "fhe", "voting"],

  "compatibleWith": ["voting"],
  "incompatibleWith": [],
  "requires": [],

  "requiresSlots": ["EVENTS", "EXTERNAL_FUNCTIONS", "STATE_VARIABLES"],

  "provides": {
    "functions": ["shareVoteCounts", "hasVoteCountAccess", "checkVoteCountFHEAccess"],
    "events": ["VoteCountAccessGranted"],
    "modifiers": [],
    "stateVariables": ["_voteCountAccessList"]
  },

  "exclusive": false,

  "injections": {
    "EVENTS": {
      "content": "file:inject/events.sol",
      "mode": "append",
      "order": 50
    },
    "STATE_VARIABLES": {
      "content": "file:inject/state.sol",
      "mode": "append",
      "order": 50
    },
    "EXTERNAL_FUNCTIONS": {
      "content": "file:inject/functions.sol",
      "mode": "append",
      "order": 50
    }
  }
}
